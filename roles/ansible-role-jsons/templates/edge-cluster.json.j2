{
  "zone_key": "{{ zone_key }}",
  "cluster_key": "edge-to-{{ serviceName }}-cluster",
  "name": "{{ serviceName }}",
  "instances": [],
  "circuit_breakers": null,
  "outlier_detection": null,
  "health_checks": [],
  {% if domain_ssl_config_status=='on' %}
  "ssl_config": {
    "require_client_certs": "{{ ssl_require_client_certs }}",
    "protocols": ["SSLv3", "SSLv2", "TLSv1", "TLSv1.1", "TLSv1.2"],
    "trust_file": "{{ ssl_config_trust_file }}",
    "cert_key_pairs": [
      {
        "certificate_path": "{{ ssl_config_certificate_path }}",
        "key_path": "{{ ssl_config_key_path }}"
      }
    ]
  },
  {% endif %}
  "require_tls": {{ edge_cluster_require_tls }}
}